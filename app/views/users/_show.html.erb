<div class="search_result">
	<p id="name"><%= user.username %></p>
	<%= image_tag user.avatar.url(:thumb), :id => "search_pic" %>
	<%= user.age %>
	<%= user.gender %>
	<div id="star_container">
		<%= image_tag "starburst.png", :id => "star" %>
		<p id="score"><%= compatibility %>%</p>
	</div>
</div>